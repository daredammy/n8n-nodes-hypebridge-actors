{
  "name": "n8n-nodes-hypebridge-actors",
  "version": "1.0.1",
  "description": "n8n community nodes for Hypebridge Apify actors - event scrapers, influencer tools, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "n8n"
  ],
  "license": "MIT",
  "author": {
    "name": "Hypebridge",
    "email": "hello@hypebridge.io"
  },
  "engines": {
    "node": ">=18.10"
  },
  "packageManager": "npm@10.8.2",
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "merge:api": "npx openapi-merge-cli --config ./openapi.config.json",
    "test": "WEBHOOK_URL=https://localhost:5678 jest --config jest.config.js",
    "create-actor-app": "npx tsx scripts/cli.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ApifyApi.credentials.js",
      "dist/credentials/ApifyOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ApifyEventbriteSearch/ApifyEventbriteSearch.node.js",
      "dist/nodes/ApifyDraftkingsPredictions/ApifyDraftkingsPredictions.node.js",
      "dist/nodes/ApifyEater/ApifyEater.node.js",
      "dist/nodes/ApifyBlindPostCommentsScraper/ApifyBlindPostCommentsScraper.node.js",
      "dist/nodes/ApifyEventnoire/ApifyEventnoire.node.js",
      "dist/nodes/ApifyInfluencerEvaluationAgentInstagramTiktok/ApifyInfluencerEvaluationAgentInstagramTiktok.node.js",
      "dist/nodes/ApifyDiceFm/ApifyDiceFm.node.js",
      "dist/nodes/ApifyPrekindle/ApifyPrekindle.node.js",
      "dist/nodes/ApifyShotgunLive/ApifyShotgunLive.node.js",
      "dist/nodes/ApifyPoshVip/ApifyPoshVip.node.js",
      "dist/nodes/ApifyInfluencerDiscoveryAgentInstagramTiktok/ApifyInfluencerDiscoveryAgentInstagramTiktok.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "apify-client": "^2.16.0",
    "chalk": "^5.6.0",
    "eslint": "^9.32.0",
    "glob": "^11.0.3",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "nock": "^14.0.5",
    "prettier": "^3.6.2",
    "sharp": "^0.34.2",
    "ts-jest": "^29.3.2",
    "typescript": "5.9.2"
  }
}
